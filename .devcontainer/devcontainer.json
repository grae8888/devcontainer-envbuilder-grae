{
    "build": {
        "dockerfile": "Dockerfile"
    },
    "onCreateCommand": "/coder-socket-access.sh",
    "runArgs": [
        "--privileged",
        "--network=host",
        "--security-opt=seccomp=unconfined",
        "-h", "${localEnv:HOSTNAME}"
    ],
    "workspaceFolder": "/workspaces",
    "remoteEnv": { "HOME": "/workspaces/devcontainer-envbuilder-grae" },
    "workspaceMount": "source=/home/coder/envbuilder,target=/workspaces,type=bind",
    "remoteUser": "root",
    "containerUser": "root",
    "mounts": [
        "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
    ],
    "containerEnv": {
        "DOCKER_TLS_CERTDIR": ""
    }
}
